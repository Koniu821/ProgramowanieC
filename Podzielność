#include <iostream>

using namespace std;

int main()
{
    int t, n, x, y, i, j;
    cin>>t;
    for (i=0; i<t; ++i)
    {
        cin>>n>>x>>y;
        for (j=0; j<n; ++j)
        {
            if ((j%x == 0) && (j%y != 0))
            {
                cout<<j<<" ";
            }
        }
        cout<<endl;
    }
    return 0;
}
